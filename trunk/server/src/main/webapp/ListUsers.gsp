<% import org.chcc.chclockin.dao.*import org.chcc.chclockin.model.* %><html><head><title>Users</title></head><body><% 	if (request.getParameter("id") != null) { 		User user = MemoryUserDao.getInstance().getUserById(request.getParameter("id"))		if (null == user) {%>User does not exist<% } else { %>id: ${user.id} <br/>name: ${user.name} <br/>ssn: ${user.ssn} <br/>payRate: ${user.payRate} <br/>role: ${user.role} <br/><% 		} %><% } else { users = MemoryUserDao.getInstance().getAllUsers()if (users.size() > 0) {users.each { user -> %><strong>${user.name}</strong> <br/>id: ${user.id} <br/>name: ${user.name} <br/>ssn: ${user.ssn} <br/>payRate: ${user.payRate} <br/>role: ${user.role} <br/><br/> <% } }else {%>No users in system<% }} %></body></html>